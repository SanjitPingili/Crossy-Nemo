<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/welcomeScreen">

    <fragment
        android:id="@+id/welcomeScreen"
        android:name="com.example.firstapptests.MainActivity"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/activity_main">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/iconsBackground" />
    </fragment>
    <fragment
        android:id="@+id/iconsBackground"
        android:name="com.example.firstapptests.Icons"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/icons">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/welcomeScreen" />
    </fragment>
</navigation>